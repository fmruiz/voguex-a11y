{
  "name": "voguex-a11y",
  "private": false,
  "version": "1.0.3",
  "type": "module",
  "description": "Voguex-A11Y is a library that provide hooks to make your components more accessible",
  "author": {
    "name": "Franco Maximiliano Ruiz",
    "email": "x.francoruiz@gmail.com"
  },
  "keywords": [
    "hooks",
    "a11y",
    "react",
    "wai-aria"
  ],
  "tags": [
    "hooks",
    "a11y",
    "react",
    "wai-aria"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fmruiz/voguex-a11y"
  },
  "main": "./dist/voguex-a11y.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/voguex-a11y.js",
      "require": "./dist/voguex-a11y.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml}\"",
    "check": "prettier --check \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml}\"",
    "prepare": "husky install",
    "release": "standad-version",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "push:npm": "np"
  },
  "peerDependencies": {
    "@types/react": "^17.0.0 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@eslint/js": "^9.1.1",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/addon-onboarding": "^8.0.9",
    "@storybook/blocks": "^8.0.9",
    "@types/dom-speech-recognition": "^0.0.4",
    "@storybook/react": "^8.0.9",
    "@storybook/react-vite": "^8.0.9",
    "@storybook/test": "^8.0.9",
    "@types/node": "^20.12.8",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "globals": "^15.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "np": "^10.0.5",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "standard-version": "^9.5.0",
    "storybook": "^8.0.9",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.8.0",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1"
  },
  "lint-staged": {
    "*.ts": [
      "npx prettier --write"
    ]
  }
}
